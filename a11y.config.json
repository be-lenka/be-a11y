{
  "rules": {
    "heading-order": true,
    "heading-empty": true,
    "alt-attributes": true,
    "redundant-title": true,
    "aria-invalid": true,
    "missing-aria": true,
    "aria-role-invalid": true,
    "missing-landmark": false,
    "contrast": true,
    "label-missing-for": true,
    "duplicate-id": true,
    "multiple-h1": true,
    "input-unlabeled": true,
    "empty-link": true,
    "iframe-title-missing": true,
    "link-new-tab-warning": true
  },
  "allowedExtensions": {
    ".latte": true,
    ".html": true,
    ".php": true,
    ".twig": true,
    ".edge": true,
    ".tsx": true,
    ".jsx": true
  },
  "excludedDirs": {
    "node_modules": true,
    "vendor": true,
    "dist": true,
    "build": true,
    "temp": true,
    ".idea": true,
    ".git": true,
    "log": true,
    "bin": true
  }
}
